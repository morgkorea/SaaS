{
    "workbench.iconTheme": "vscode-icons",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.suggestSelection": "first",
    "workbench.colorTheme": "One Dark Pro Mix",
    "tabnine.experimentalAutoImports": true,
    "workbench.settings.openDefaultSettings": true,
    "editor.colorDecorators": true,
    "editor.codeActionsOnSave": {},
    "editor.fontFamily": "JetBrains Mono, Proggy,  Hack,   ",
    "editor.fontSize": 12,
    "explorer.confirmDelete": false,
    "editor.tokenColorCustomizations": {
        "comments": "#807c5c"
    },
    "json.schemas": [],
    "prettier.singleQuote": true,
    "auto-rename-tag.activationOnLanguage": [
        "*"
    ],
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "workbench.startupEditor": "none",
    "window.zoomLevel": 1,
    "prettier.bracketSameLine": true,
    "prettier.requireConfig": true,
    "prettier.requirePragma": true,
    "editor.wordWrap": "on",
    "diffEditor.wordWrap": "on",
    "eslint.nodePath": ".yarn/sdks",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "workbench.colorCustomizations": {
        "terminal.background": "#02263b",
        "terminal.foreground": "#02263b",
        "terminalCursor.background": "#d5ef7f",
        "terminalCursor.foreground": "#d5ef7f",
        "terminal.ansiBlack": "#e8c00b",
        "terminal.ansiBlue": "#e8c00b",
        "terminal.ansiBrightBlack": "#4C566A",
        "terminal.ansiBrightBlue": "#EBCB8B",
        "terminal.ansiBrightCyan": "#914d38",
        "terminal.ansiBrightGreen": "#BF616A",
        "terminal.ansiBrightMagenta": "#A3BE8C",
        "terminal.ansiBrightRed": "#88C0D0",
        "terminal.ansiBrightWhite": "#8FBCBB",
        "terminal.ansiBrightYellow": "#5E81AC",
        "terminal.ansiCyan": "#D08770",
        "terminal.ansiGreen": "#BF616A",
        "terminal.ansiMagenta": "#A3BE8C",
        "terminal.ansiRed": "#88C0D0",
        "terminal.ansiWhite": "#E5E9F0",
        "terminal.ansiYellow": "#5E81AC"
    },
    "git.autofetch": true,
    "materialTheme.accent": "Tomato",
    "prettier.insertPragma": true,
    "prettier.resolveGlobalModules": true,
    "editor.formatOnSave": true,
    "editor.experimental.asyncTokenization": true,
    "editor.quickSuggestions": {
        "strings": "inline"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.multiCursorModifier": "ctrlCmd",
    "auto-close-tag.fullMode": true,
    "eslint.experimental.useFlatConfig": true,
    "eslint.execArgv": null,
    "eslint.format.enable": true,
    "eslint.ignoreUntitled": true,
    "eslint.lintTask.enable": true,
    "eslint.nodeEnv": "",
    "eslint.onIgnoredFiles": "warn",
    "eslint.problems.shortenToSingleLine": true,
    "eslint.quiet": true,
    "vs-code-prettier-eslint.prettierLast": true,
    "prettier.jsxSingleQuote": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "files.autoSave": "off",
    "oneDarkPro.editorTheme": "One Dark Pro Flat",
    "oneDarkPro.vivid": true,
    "oneDarkPro.bold": true,
    "terminal.integrated.fontFamily": "D2Coding, JetBrains Mono,Proggy, Hack ",
}